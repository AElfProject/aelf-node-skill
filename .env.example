# EOA private key for write operations
AELF_PRIVATE_KEY=

# Optional chain-level RPC overrides
AELF_NODE_AELF_RPC_URL=
AELF_NODE_TDVV_RPC_URL=

# Optional custom node registry path (default: ~/.aelf-node-skill/nodes.json)
AELF_NODE_REGISTRY_PATH=

# Optional request timeout in ms
AELF_NODE_TIMEOUT_MS=10000

# Optional retries for REST calls
AELF_NODE_RETRY=1

# Optional cache limits
AELF_SDK_INSTANCE_CACHE_MAX=32
AELF_SDK_CONTRACT_CACHE_MAX=256
AELF_REST_CLIENT_CACHE_MAX=64
