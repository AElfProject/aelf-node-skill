{
  "name": "@aelfproject/aelf-node-skill",
  "version": "0.1.0",
  "description": "AElf Node Skill for AI agents: MCP, CLI, and SDK with SDK-first + REST fallback architecture.",
  "type": "module",
  "main": "index.ts",
  "exports": {
    ".": "./index.ts",
    "./mcp": "./src/mcp/server.ts"
  },
  "bin": {
    "aelf-node-skill": "./aelf_node_skill.ts",
    "aelf-node-setup": "./bin/setup.js"
  },
  "files": [
    "index.ts",
    "src/",
    "lib/",
    "bin/",
    "aelf_node_skill.ts",
    "openclaw.json",
    "mcp-config.example.json",
    "README.md",
    "README.zh-CN.md",
    "LICENSE",
    ".env.example"
  ],
  "scripts": {
    "setup": "bun run bin/setup.ts",
    "mcp": "bun run src/mcp/server.ts",
    "cli": "bun run aelf_node_skill.ts",
    "test": "bun test tests/",
    "test:unit": "bun test tests/unit/",
    "test:unit:coverage": "bun run test:unit --coverage --coverage-reporter=text --coverage-reporter=lcov --coverage-dir=coverage",
    "coverage:core": "bun run bin/check-core-coverage.ts",
    "test:unit:coverage:gate": "bun run test:unit:coverage && bun run coverage:core",
    "test:integration": "bun test tests/integration/",
    "test:live": "RUN_LIVE_TESTS=1 bun test tests/integration/"
  },
  "keywords": [
    "aelf",
    "mcp",
    "agent",
    "blockchain",
    "sdk",
    "skill"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AElfProject/aelf-node-skill.git"
  },
  "homepage": "https://github.com/AElfProject/aelf-node-skill#readme",
  "bugs": {
    "url": "https://github.com/AElfProject/aelf-node-skill/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.26.0",
    "aelf-sdk": "3.5.1-beta.0",
    "commander": "^12.1.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.7.0"
  }
}
